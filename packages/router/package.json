{
  "name": "@effect-x/router",
  "version": "0.0.7",
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "directory": "lib"
  },
  "files": [
    "lib",
    "!lib/**/*.map"
  ],
  "engines": {
    "node": ">=14.15.0"
  },
  "scripts": {
    "build": "build-util build --configPath=../../scripts/pkg/pack.js",
    "build:watch": "build-util build --configPath=../../scripts/pkg/pack.js --watch",
    "lint": "eslint ./src",
    "test": "jest"
  },
  "exports": {
    ".": {
      "module": "./lib/_esm/index.js",
      "default": "./lib/index.js"
    },
    "./*": {
      "module": "./lib/_esm/*.js",
      "default": "./lib/*.js"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "lib/_types/*",
        "lib/_types/*/index.d.ts"
      ]
    }
  },
  "devDependencies": {
    "@effect-x/build-util": "^1.0.0",
    "@types/react": "^17.0.24",
    "@types/react-router-dom": "^5.3.0",
    "path-to-regexp": "^1.7.0",
    "qs": "^6.10.1",
    "ramda": "^0.27.1",
    "react": "^17.0.2",
    "react-router-dom": "^5.3.0"
  },
  "peerDependencies": {
    "path-to-regexp": "^1.7.0",
    "qs": "^6.10.1",
    "ramda": ">=0.27",
    "react": ">=17",
    "react-router-dom": "^5.2.0"
  },
  "eslintConfig": {
    "ignorePatterns": [
      "lib/"
    ],
    "extends": [
      "@effect-x/eslint-config",
      "@effect-x/eslint-config/react"
    ],
    "rules": {
      "@typescript-eslint/no-explicit-any": "off",
      "react/react-in-jsx-scope": "error",
      "react/jsx-uses-react": "error"
    }
  }
}
